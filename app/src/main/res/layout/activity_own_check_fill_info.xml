            android:textColor="@color/color_text"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@id/aocf_tv_reservation_date"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/aocf_tv_reservation_date"
            app:layout_constraintTop_toTopOf="@id/aocf_tv_reservation_date" />

