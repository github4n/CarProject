            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/aocf_tv_driver_name"
            app:layout_constraintTop_toTopOf="@id/aocf_tv_driver_name" />

        <ImageView
            android:id="@+id/aocf_line3"
            android:layout_width="0dp"
            android:layout_height="1px"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="@color/color_sub_line"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/aocf_et_driver_name" />

        <TextView
            android:id="@+id/aocf_tv_driver_brand"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="12dp"
            android:text="@string/text_driver_brand"
            android:textSize="12sp"
            android:textColor="@color/color_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/aocf_line3" />

        <EditText
            android:id="@+id/aocf_et_driver_brand"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:background="@null"
            android:textColor="@color/color_text"
            android:textSize="12sp"
            android:gravity="end"
            app:layout_constraintBottom_toBottomOf="@id/aocf_tv_driver_brand"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/aocf_tv_driver_brand"
            app:layout_constraintTop_toTopOf="@id/aocf_tv_driver_brand" />

        <ImageView
            android:id="@+id/aocf_line5"
            android:layout_width="0dp"
            android:layout_height="1px"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="@color/color_sub_line"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/aocf_et_driver_brand" />

        <TextView
            android:id="@+id/aocf_tv_driver_plate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="12dp"
            android:text="@string/text_driver_plate"
            android:textSize="12sp"
            android:textColor="@color/color_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/aocf_line5" />

        <EditText
            android:id="@+id/aocf_et_driver_plate"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:background="@null"
            android:textColor="@color/color_text"
            android:textSize="12sp"
            android:gravity="end"
            app:layout_constraintBottom_toBottomOf="@id/aocf_tv_driver_plate"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/aocf_tv_driver_plate"
            app:layout_constraintTop_toTopOf="@id/aocf_tv_driver_plate" />

        <ImageView
            android:id="@+id/aocf_line6"
            android:layout_width="0dp"
            android:layout_height="1px"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="@color/color_sub_line"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/aocf_et_driver_plate" />

        <TextView
            android:id="@+id/aocf_tv_car_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="12dp"
            android:text="@string/text_car_type"
            android:textSize="12sp"
            android:textColor="@color/color_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/aocf_line6" />

        <TextView
            android:id="@+id/aocf_et_car_type"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="16dp"
            android:layout_marginStart="8dp"
            android:drawableEnd="@drawable/acf_more"
            android:drawableRight="@drawable/acf_more"
            android:gravity="center_horizontal"
            android:textColor="@color/color_text"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@id/aocf_tv_car_type"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/aocf_tv_car_type"
            app:layout_constraintTop_toTopOf="@id/aocf_tv_car_type" />

        <ImageView
            android:id="@+id/aocf_line7"
            android:layout_width="0dp"
            android:layout_height="1px"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="@color/color_sub_line"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/aocf_et_car_type" />

        <TextView
            android:id="@+id/aocf_tv_pick_reservation"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@color/color_sub_background"
            android:paddingBottom="8dp"
            android:paddingTop="8dp"
            android:paddingLeft="8dp"
            android:text="@string/text_pick_reservation"
            android:textColor="@color/color_text"
            android:textSize="13sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/aocf_tv_car_type" />

        <TextView
            android:id="@+id/aocf_tv_pick_station"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="12dp"
            android:text="@string/text_pick_station"
            android:textSize="12sp"
            android:textColor="@color/color_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/aocf_tv_pick_reservation" />

        <TextView
            android:id="@+id/aocf_et_pick_station"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="16dp"
            android:layout_marginStart="8dp"
