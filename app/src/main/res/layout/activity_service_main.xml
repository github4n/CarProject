            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/sm_img_info"
            app:layout_constraintTop_toTopOf="@id/sm_cl_guideline9" />

    </android.support.constraint.ConstraintLayout>

